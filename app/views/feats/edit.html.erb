<h1>Editing feat</h1>

<%= error_messages_for :feat %>

<% form_for(@feat) do |f| %>
  <p>
    <b>Character</b><br />
    <%= f.text_field :character %>
  </p>

  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @feat %> |
<%= link_to 'Back', feats_path %>
